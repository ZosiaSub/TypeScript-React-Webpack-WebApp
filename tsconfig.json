{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": "src",    
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "jsx": "react",
    "lib": ["es6", "dom"],
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "typeRoots": ["./node_modules/@types", "./custom_typings"]
  }
}
